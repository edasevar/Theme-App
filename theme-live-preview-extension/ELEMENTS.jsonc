	// =================================================
	// SEMANTIC TOKEN COLORS
	// =================================================
	"semanticTokenColors": {
		// --- Types ---
		"class": "#0013EA",
		"interface": "#0013EA",
		"enum": "#0013EA",
		"struct": "#0013EA",
		"type": "#0013EA",
		"typeParameter": "#0013EA",
		// --- Variables ---
		"variable": "#F6FCFC",
		"parameter": "#F6FCFC",
		"property": "#F6FCFC",
		"enumMember": "#F6FCFC",
		// --- Values and Literals ---
		"string": "#F904CC",
		"number": "#1BBA04",
		"boolean": "#1BBA04",
		"regexp": "#F904CC",
		// --- Functions ---
		"function": "#0013EA",
		"method": "#E4F403",
		// --- Keywords ---
		"keyword": "#DA0000",
		"modifier": "#DA0000",
		"namespace": "#0013EA",
		// --- Comments ---
		"comment": "#B6B6B6",
		// --- Special Modifiers ---
		"*.static": {
			"fontStyle": "italic"
		},
		"*.readonly": {
			"fontStyle": "bold"
		},
		"*.decorator": {
			"fontStyle": "italic",
			"foreground": "#0013EA"
		},
		"*.abstract": {
			"fontStyle": "italic"
		}
	},
	// =================================================
	// TOKEN COLORS (TEXTMATE)
	// =================================================
	"tokenColors": [
		// --- Base Text & Structure ---
		{
			"scope": [
				"source",
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		{
			"scope": [
				"punctuation",
				"punctuation.terminator",
				"punctuation.definition.tag",
				"punctuation.separator",
				"punctuation.definition.string",
				"punctuation.section.block"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		// --- Classes & Types ---
		{
			"scope": [
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#0013EA"
			}
		},
		{
			"scope": [
				"entity.name.type.interface",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#0013EA"
			}
		},
		{
			"scope": [
				"entity.name.type.struct"
			],
			"settings": {
				"foreground": "#0013EA"
			}
		},
		{
			"scope": [
				"entity.name.type.enum"
			],
			"settings": {
				"foreground": "#0013EA"
			}
		},
		{
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#E4F403"
			}
		},
		{
			"scope": [
				"variable.type.parameter",
				"variable.parameter.type"
			],
			"settings": {
				"foreground": "#0013EA"
			}
		},
		// --- Functions & Methods ---
		{
			"scope": [
				"entity.name.function.method",
				"meta.function.method"
			],
			"settings": {
				"foreground": "#E4F403"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call.generic"
			],
			"settings": {
				"foreground": "#0013EA"
			}
		},
		{
			"scope": [
				"variable.function"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		{
			"scope": [
				"entity.name.function.preprocessor",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#E4F403"
			}
		},
		{
			"scope": [
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#F904CC"
			}
		},
		{
			"scope": [
				"meta.decorator",
				"punctuation.decorator",
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#0013EA"
			}
		},
		// --- Variables & Properties ---
		{
			"scope": [
				"variable",
				"meta.variable",
				"variable.other.object.property",
				"variable.other.readwrite.alias"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		{
			"scope": [
				"variable.other.object"
			],
			"settings": {
				"foreground": "#E4F403"
			}
		},
		{
			"scope": [
				"variable.other.global",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#F904CC"
			}
		},
		{
			"scope": [
				"variable.other.local"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		{
			"scope": [
				"variable.parameter",
				"meta.parameter"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		{
			"scope": [
				"variable.other.property",
				"meta.property"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		{
			"scope": [
				"variable.other.constant",
				"variable.readonly"
			],
			"settings": {
				"foreground": "#E4F403"
			}
		},
		{
			"scope": [
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		// --- Keywords & Operators ---
		{
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#DA0000"
			}
		},
		{
			"scope": [
				"keyword.control.import",
				"keyword.control.from",
				"keywaord.import"
			],
			"settings": {
				"foreground": "#DA0000"
			}
		},
		{
			"scope": [
				"keyword.control.exception",
				"keyword.control.trycatch"
			],
			"settings": {
				"foreground": "#DA0000"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.modifier",
				"storage.type"
			],
			"settings": {
				"foreground": "#DA0000"
			}
		},
		{
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#DA0000"
			}
		},
		// --- Strings & Literals ---
		{
			"scope": [
				"string",
				"string.other.link",
				"markup.inline.raw.string.markdown"
			],
			"settings": {
				"foreground": "#F904CC"
			}
		},
		{
			"scope": [
				"constant.character.escape",
				"constant.other.placeholder"
			],
			"settings": {
				"foreground": "#F6FCFC"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#1BBA04"
			}
		},
		{
			"scope": [
				"constant.language.boolean",
				"constant.language.json"
			],
			"settings": {
				"foreground": "#1BBA04"
			}
		},
		{
			"scope": [
				"entity.name.label",
				"punctuation.definition.label"
			],
			"settings": {
				"foreground": "#F904CC"
			}
		},
		// --- Comments ---
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#B6B6B6"
			}
		},
		{
			"scope": [
				"comment.documentation",
				"comment.line.documentation"
			],
			"settings": {
				"foreground": "#B6B6B6"
			}
		},
